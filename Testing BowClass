const { Bow } = require('./projet_OOP');
const testBow = () =>
{
    let bow = new Bow('Bow', 20, 30, 'High', 3); 
    for (let i = 0; i < 4; i++) 
    { 
        bow.attack();

    }
    console.log(`Needs ammunition: ${bow.needsAmmunition()}`);
    bow.prepareForAttack();
    console.log('');
}

testBow();
